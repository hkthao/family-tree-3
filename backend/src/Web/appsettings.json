{
  "AI": {
    "Provider": "Gemini", // Default AI provider (Gemini, OpenAI, LocalAI)
    "MaxTokensPerRequest": 500,
    "DailyUsageLimit": 10000,
    "Gemini": {
      "ApiKey": "YOUR_GEMINI_API_KEY"
    },
    "OpenAI": {
      "ApiKey": "YOUR_OPENAI_API_KEY",
      "Organization": "YOUR_OPENAI_ORGANIZATION_ID"
    },
    "LocalAI": {
      "Endpoint": "http://localhost:8080/v1"
    }
  },
  "Auth0": {
    "Domain": "YOUR_AUTH0_DOMAIN",
    "Audience": "YOUR_AUTH0_AUDIENCE",
    "Namespace": "https://familytree.com/"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=mysql;Port=3306;Database=familytree_db;Uid=root;Pwd=root_password;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
