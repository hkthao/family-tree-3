{
  "AIChatSettings": {
    "Provider": "Gemini",
    "MaxTokensPerRequest": 500,
    "DailyUsageLimit": 10000,
    "Providers": {
      "Gemini": {
        "ApiKey": "YOUR_GEMINI_API_KEY",
        "Model": "gemini-pro"
      },
      "OpenAI": {
        "ApiKey": "YOUR_OPENAI_API_KEY",
        "Model": "gpt-3.5-turbo"
      },
      "LocalAI": {
        "Endpoint": "http://localhost:8080/v1"
      }
    }
  },
  "EmbeddingSettings": {
    "Provider": "OpenAI",
    "MaxTokensPerRequest": 0,
    "DailyUsageLimit": 0,
    "Providers": {
      "Pinecone": {
        "ApiKey": "YOUR_PINECONE_API_KEY",
        "Environment": "YOUR_PINECONE_ENVIRONMENT",
        "IndexName": "YOUR_PINECONE_INDEX_NAME"
      }
    }
  },
  "VectorStoreSettings": {
    "Provider": "Pinecone",
    "Pinecone": {
      "ApiKey": "YOUR_PINECONE_API_KEY",
      "Environment": "YOUR_PINECONE_ENVIRONMENT",
      "IndexName": "YOUR_PINECONE_INDEX_NAME"
    }
  },
  "DefaultConnection": "Server=localhost;Port=3306;Database=familytree_db;Uid=root;Pwd=root_password;",
  "JwtSettings": {
    "Authority": "YOUR_AUTH0_DOMAIN",
    "Audience": "YOUR_AUTH0_AUDIENCE",
    "Namespace": "https://familytree.com/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "StorageSettings": {
    "Provider": "Local",
    "MaxFileSizeMB": 5,
    "Local": {
      "LocalStoragePath": "uploads",
      "BaseUrl": "http://localhost:8080"
    },
    "Cloudinary": {
      "CloudName": "YOUR_CLOUDINARY_CLOUD_NAME",
      "ApiKey": "YOUR_CLOUDINARY_API_KEY",
      "ApiSecret": "YOUR_CLOUDINARY_API_SECRET"
    },
    "S3": {
      "BucketName": "YOUR_S3_BUCKET_NAME",
      "AccessKey": "YOUR_S3_ACCESS_KEY",
      "SecretKey": "YOUR_S3_SECRET_KEY",
      "Region": "YOUR_S3_REGION"
    }
  },
  "FaceDetectionService": {
    "BaseUrl": "http://localhost:8000"
  }
}
