{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DefaultAiProvider": "LocalLLM", // Can be "Gemini", "OpenAI", or "LocalLLM"
  "FamilyTreeBackend": {
    "BaseUrl": "http://backend:8080", // Internal Docker service name and port
    "JwtAudience": "family-tree-api", // This should match the audience configured in the main backend's JWT settings
    "JwtIssuer": "family-tree-auth-server" // This should match the issuer configured in the main backend's JWT settings
  },
  "Gemini": {
    "ApiKey": "YOUR_GEMINI_API_KEY", // Replace with your actual Gemini API Key
    "Model": "gemini-pro" // Or another Gemini model
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY", // Replace with your actual OpenAI API Key
    "Model": "gpt-3.5-turbo" // Or another OpenAI model
  },
  "LocalLLM": {
    "BaseUrl": "http://localhost:11434/api/generate", // Example for Ollama default
    "Model": "llama3:8b" // Example for Ollama model
  }
}