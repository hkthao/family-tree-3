{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage",
    "test:coverage": "vitest run --coverage --coverage.threshold.statements=80 --coverage.threshold.branches=80 --coverage.threshold.functions=80 --coverage.threshold.lines=80",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../../.gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../../.gitignore"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.5.0",
    "@auth0/auth0-vue": "^2.4.0",
    "@mdi/font": "^7.4.47",
    "@novu/js": "^3.11.0",
    "@pinia/testing": "^1.0.2",
    "apexcharts": "^5.3.5",
    "axios": "^1.7.2",
    "d3": "^7.9.0",
    "family-chart": "^0.8.0",
    "pinia": "^3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass-embedded": "^1.77.0",
    "vue": "^3.5.18",
    "vue-advanced-cropper": "^2.8.9",
    "vue-editor-js": "^2.0.2",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "vue3-lottie": "^3.3.1",
    "vuetify": "^3.10.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.3.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4",
    "@playwright/test": "^1.45.0"
  },
  "overrides": {
    "minimatch": "9.0.5"
  }
}
