{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AIChatSettings": {
    "Provider": "${AIChatSettings_Provider}",
    "ScoreThreshold": "${AIChatSettings_ScoreThreshold}",
    "Gemini": {
      "ApiKey": "${AIChatSettings_Gemini_ApiKey}",
      "Model": "${AIChatSettings_Gemini_Model}",
      "Region": "${AIChatSettings_Gemini_Region}"
    },
    "OpenAI": {
      "ApiKey": "${AIChatSettings_OpenAI_ApiKey}",
      "Model": "${AIChatSettings_OpenAI_Model}",
      "Organization": "${AIChatSettings_OpenAI_Organization}",
      "MaxTextLength": "${AIChatSettings_OpenAI_MaxTextLength}"
    },
    "Local": {
      "ApiUrl": "${AIChatSettings_Local_ApiUrl}",
      "Model": "${AIChatSettings_Local_Model}",
      "MaxTextLength": "${AIChatSettings_Local_MaxTextLength}"
    }
  },
  "EmbeddingSettings": {
    "Provider": "${EmbeddingSettings_Provider}",
    "OpenAI": {
      "ApiKey": "${EmbeddingSettings_OpenAI_ApiKey}",
      "Model": "${EmbeddingSettings_OpenAI_Model}",
      "Organization": "${EmbeddingSettings_OpenAI_Organization}",
      "MaxTextLength": "${EmbeddingSettings_OpenAI_MaxTextLength}"
    },
    "Cohere": {
      "ApiKey": "${EmbeddingSettings_Cohere_ApiKey}",
      "Model": "${EmbeddingSettings_Cohere_Model}",
      "MaxTextLength": "${EmbeddingSettings_Cohere_MaxTextLength}"
    },
    "Local": {
      "ApiUrl": "${EmbeddingSettings_Local_ApiUrl}",
      "Model": "${EmbeddingSettings_Local_Model}",
      "MaxTextLength": "${EmbeddingSettings_Local_MaxTextLength}"
    }
  },
  "VectorStoreSettings": {
    "Provider": "${VectorStoreSettings_Provider}",
    "TopK": "${VectorStoreSettings_TopK}",
    "Pinecone": {
      "ApiKey": "${VectorStoreSettings_Pinecone_ApiKey}",
      "Host": "${VectorStoreSettings_Pinecone_Host}",
      "IndexName": "${VectorStoreSettings_Pinecone_IndexName}"
    },
    "Qdrant": {
      "Host": "${VectorStoreSettings_Qdrant_Host}",
      "ApiKey": "${VectorStoreSettings_Qdrant_ApiKey}",
      "CollectionName": "${VectorStoreSettings_Qdrant_CollectionName}",
      "VectorSize": "${VectorStoreSettings_Qdrant_VectorSize}"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${ConnectionStrings_DefaultConnection}"
  },
  "JwtSettings": {
    "Authority": "${JwtSettings_Authority}",
    "Audience": "${JwtSettings_Audience}",
    "Namespace": "${JwtSettings_Namespace}"
  },
  "StorageSettings": {
    "Provider": "${StorageSettings_Provider}",
    "BaseUrl": "${StorageSettings_BaseUrl}",
    "MaxFileSizeMB": "${StorageSettings_MaxFileSizeMB}",
    "Local": {
      "LocalStoragePath": "${StorageSettings_Local_LocalStoragePath}",
      "BaseUrl": "${StorageSettings_Local_BaseUrl}"
    },
    "Cloudinary": {
      "CloudName": "${StorageSettings_Cloudinary_CloudName}",
      "ApiKey": "${StorageSettings_Cloudinary_ApiKey}",
      "ApiSecret": "${StorageSettings_Cloudinary_ApiSecret}"
    },
    "S3": {
      "BucketName": "${StorageSettings_S3_BucketName}",
      "AccessKey": "${StorageSettings_S3_AccessKey}",
      "SecretKey": "${StorageSettings_S3_SecretKey}",
      "Region": "${StorageSettings_S3_Region}"
    }
  },
  "FaceDetectionService": {
    "BaseUrl": "${FaceDetectionSettings_BaseUrl}"
  },
  "CORS_ORIGINS": "${CORS_ORIGINS}",
  "UseInMemoryDatabase": "${UseInMemoryDatabase}"
}
